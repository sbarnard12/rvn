<div class="container-fluid" style="background: #c7e6b5;max-width: 100%; font-weight: bold" >
		<ul class="nav navbar-nav">
        	<li><a id="reviews_nav" name={{user._id}}>Reviews</a></li>
        	<li><a id="taskHistory_nav" name={{user._id}}>Task History</a></li>
        	<li><a id="currentTasks_nav" name={{user._id}}>Current Tasks</a></li>
        	<li class="active"><a id="profile_nav" name={{user._id}}>Profile</a></li>
    	</ul>
</div>
<div id="partial_container">

<div id="reviews" class="tab active">
    <br>
    <div class= "container-fluid" style=" max-width: 80%; background: #9bd17d">
        <h1>Reviews</h1>
    </div>
    <br>
    <div class= "container-fluid" style=" max-width: 80%;">
        {{#each reviews}}
            <table class="taskhistory_table" border="2">
                <tr style="background-color:#ececec">
                    <th>Reviewed By: </th>
                    <td>{{this.reviewer.name}}</td>
                </tr>
                <tr>
                    <th>Review Date</th>
                    <td>{{this.dateString}}</td>
                </tr>
                <tr style="background-color:#ececec">
                    <th>Comments</th>
                    <td>{{this.description}}</td>
                </tr>
            </table>
            <br>
        {{/each}}
    </div>

</div>

</div>
<script src="/javascripts/userProfileScript.js"></script>